#include "main.h"
 /**
 * _strcat : concatenates two strings
 * @dest: input value
 * @src: input value
 * description:  strcating strings This function takes two null-terminated strings, `str1`
 *concatenates `str2` to t.
 * Return: void
 */
char *_strcat(char *dest, char *src)
{
	int i;
	int j;


	i = 0;
	while (dest[i] != '\0')
	{
		i++;
	}
	j = 0;
	while (src[j] != '\0')
	{
		dest[i] = src[j];
		i++;
		j++;
	}


	dest[i] = '\0';
	return (dest);
}
